<% @prices["DISPLAY"].each do |key, value| %>
  <%= key %> - <%= value["EUR"]["PRICE"] %><br>
  24 High: <%= value["EUR"]["HIGHDAY"] %><br>
  24 Low: <%= value["EUR"]["LOWDAY"] %><br>
  Market Cap: <%= value["EUR"]["MKTCAP"] %><br>
  <br>
<% end %>

<div class="contatiner">
  <div class="row">
    <% for x in @news["Data"] %>
      <div class="col-sm">
        <div class="card" style="width: 18rem;">
          <img src="<%= x['imageurl'] %>" class="" alt="<%= x['source'] %>">
          <div class="card-body">
            <h5 class="card-title"><%= x['title'] %></h5>
            <p class="card-text"><%= x['body'] %></p>
            <a href="<%= x['url'] %>" class="btn btn-secondary">Read More...</a>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>